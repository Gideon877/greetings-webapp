<title>Admin Settings</title>
</head>

<body>
    <div class="ui middle aligned center aligned container">
        <hr>
        <div class="ui piled segment">
            <h4>Languages</h4>
            <div class="ui container">
                <div class="ui labeled icon menu" id="you can remove, compact, labeled">
                    <div class="item">
                       <i class="language icon"></i>
                        Languages
                    </div>
                    <div class="item">
                        <i class="users icon"></i>
                        <a href="/admin">Names</a>
                    </div>
                    <div class="right item">
                        <i class="home icon"></i>
                        <a href="/">Home</a>
                    </div>
                </div>
            </div>
        </div>
        <div class="ui tall stacked segment">
            <h3><u><b>List of all languages available.</b></u></h3>
            <div class="ui container">
                <div class="ui relaxed horizontal list">
                    {{#each languages}}
                    <div class="item">
                        <div class="content">
                            <div class="header"><i class="{{this.flag}} flag"></i> {{this.language}}</div>
                        </div>
                    </div>
                    {{/each}}
                </div>
            </div>
        </div>
    </div>
</body>


